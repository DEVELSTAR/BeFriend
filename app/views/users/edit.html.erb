<%= render 'layouts/home_back_btn' %>
<%= form_with model: current_user, local: true do |f| %>
  <%= f.label :name %><br>
    <%= f.text_field :name %><br>
  <%= f.label :bio %><br>
    <%= f.text_area :bio %><br>
  <%= f.label :email %><br>
    <%= f.text_field :email %><br>
  <%= f.label :phone %><br>
    <%= f.text_field :phone %><br>
    <%= f.submit "Submit" %>
<% end %>
<%= link_to 'Back', :back %>
